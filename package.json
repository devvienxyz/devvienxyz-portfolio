{
	"name": "devvienxyz",
	"version": "1.0.0",
	"private": true,
	"scripts": {
		"format-check-all": "bash ./scripts/format.sh format_check_all",
		"format-check-staged": "bash ./scripts/format.sh format_check_staged",
		"format-fix-all": "bash ./scripts/format.sh format_fix_all",
		"format-fix-staged": "bash ./scripts/format.sh format_fix_staged",
		"test": "vitest --watch",
		"test:ui": "vitest --watch --ui",
		"dev": "pnpm --filter @portfolio dev",
		"build:all": "pnpm -r --filter \"!@shared\" build",
		"build": "pnpm --filter @portfolio build",
		"postbuild": "node scripts/merge-dist.js",
		"deploy": "gh-pages -d dist-root",
		"release": "pnpm run build && pnpm run postbuild && pnpm run deploy",
		"prepare": "husky"
	},
	"devDependencies": {
		"@biomejs/biome": "1.9.4",
		"@eslint/js": "^9.25.1",
		"@testing-library/react": "^16.3.0",
		"@vitejs/plugin-react-swc": "^3.9.0",
		"@vitest/ui": "3.1.2",
		"autoprefixer": "^10.4.21",
		"eslint": "^9.25.1",
		"eslint-plugin-react-hooks": "^5.2.0",
		"eslint-plugin-react-refresh": "^0.4.20",
		"fs-extra": "^11.3.0",
		"gh-pages": "^6.3.0",
		"husky": "^9.1.7",
		"jsdom": "^26.1.0",
		"postcss": "^8.5.3",
		"tailwindcss": "^4.1.4",
		"vite": "^6.3.2",
		"vite-plugin-pwa": "^1.0.0",
		"vitest": "^3.1.2"
	},
	"dependencies": {
		"@tailwindcss/vite": "^4.1.4",
